<view class="container"><block wx:if="{{flag}}"><view class="review"><block wx:for="{{reviewList}}" wx:for-item="item" wx:for-index="__i0__" wx:key="id"><view class="reviewList"><view class="roomID">{{"会议室ID"+item.meetingRoomId}}</view><view class="name">{{item.meetingName}}</view><view class="time">{{item.startTime+"~"+item.endTime}}</view><view class="buttons"><view data-event-opts="{{[['tap',[['undo',['$0'],[[['reviewList','id',item.id,'id']]]]]]]}}" class="undo" bindtap="__e">撤销预约</view><view data-event-opts="{{[['tap',[['update',['$0','$1','$2','$3','$4','$5'],[[['reviewList','id',item.id,'startTime']],[['reviewList','id',item.id,'id']],[['reviewList','id',item.id,'meetingName']],[['reviewList','id',item.id,'meetingRoomId']],[['reviewList','id',item.id,'name']],[['reviewList','id',item.id,'phone']]]]]]]}}" class="update" bindtap="__e">修改预约时间</view></view></view></block><block wx:if="{{isOver}}"><view class="isOver">---我已经到底啦---</view></block></view></block><block wx:if="{{!flag}}"><view class="no">--没有审核中的预约--</view></block></view>