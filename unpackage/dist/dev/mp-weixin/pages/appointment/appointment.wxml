<view class="container"><view class="content"><view class="title">{{"会议室："+number}}</view><view class="time"><text class="text">预约时间</text><text class="timeValue">{{fulltime}}</text></view><view class="name"><text class="text">会议名称</text><input class="nameValue" placeholder="请输入内容" placeholder-style="margin-left:10rpx" data-event-opts="{{[['input',[['nameInput',['$event']]]]]}}" bindinput="__e"/></view><view class="phone"><text class="text">联系电话</text><input class="phoneValue" type="number" data-event-opts="{{[['input',[['phoneInput',['$event']]]]]}}" bindinput="__e"/></view><view class="description"><text class="text">会议内容</text><input data-event-opts="{{[['input',[['descriptionInput',['$event']]]]]}}" class="descriptionValue" bindinput="__e"/></view><view data-event-opts="{{[['tap',[['toAppointment']]]]}}" class="button" bindtap="__e">立刻预约</view></view></view>