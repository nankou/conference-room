.container {
  background-color: #e1efff;
}
.container .no {
  height: 800rpx;
  padding: 30rpx;
  text-align: center;
  color: #999999;
}
.container .title {
  background-color: #FFFFFF;
  font-size: 35rpx;
  font-weight: 600;
  padding: 25rpx;
  font-family: PingFangSC;
  font-style: normal;
  padding-left: 25rpx;
}
.container .content {
  padding-bottom: 30rpx;
}
.container .content .tagging {
  font-size: 25rpx;
  padding: 15rpx;
  padding-left: 53rpx;
  color: #6b6b6b;
}
.container .content .searchBox {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  padding-left: 20rpx;
}
.container .content .searchBox .searchImg {
  width: 35rpx;
  height: 35rpx;
  margin-left: 10rpx;
}
.container .content .date, .container .content .time {
  width: 300rpx;
  height: 50rpx;
  background-color: #FFFFFF;
  margin: 10rpx;
  margin-top: 40rpx;
  margin-bottom: 40rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  border-radius: 10rpx;
}
.container .content .date .picker, .container .content .time .picker {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.container .content .date .picker .uni-input, .container .content .time .picker .uni-input {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  font-size: 22rpx;
  color: #6d6d6d;
  padding-left: 20rpx;
}
.container .content .date .picker .uni-input .dateImg, .container .content .time .picker .uni-input .dateImg {
  width: 23rpx;
  height: 23rpx;
  padding: 16rpx;
  margin-left: 90rpx;
}
.container .content .date .picker .uni-input .timeImg, .container .content .time .picker .uni-input .timeImg {
  width: 23rpx;
  height: 23rpx;
  padding: 16rpx;
  margin-left: 102rpx;
}
.container .content .conference {
  margin-left: 10rpx;
}
.container .content .conference .conferenceList {
  width: 645rpx;
  height: 335rpx;
  background-color: #FFFFFF;
  margin: 40rpx;
  border-radius: 15rpx;
}
.container .content .conference .conferenceList .roomID {
  padding: 40rpx;
}
.container .content .conference .conferenceList .text {
  padding-left: 40rpx;
  padding-top: 15rpx;
  font-size: 25rpx;
  color: #a1a1a1;
}
.container .content .conference .conferenceList .button {
  background-color: #fad3dd;
  margin-top: 100rpx;
  height: 80rpx;
  border-bottom-left-radius: 15rpx;
  border-bottom-right-radius: 15rpx;
  color: #6d6668;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  font-size: 28rpx;
}
.container .content .isOver {
  text-align: center;
  color: #999999;
  margin-bottom: 20rpx;
}

